@model List<UserManagement.Models.User>

<table class="table">
    <thead>
        <tr>
            <th scope="col">User ID</th>
            <th scope="col">User Name</th>          
            <th scope="col">Phone Number</th>
            <th scope="col">Email</th>
            
        </tr>
    </thead>
    <tbody>
            @foreach(UserManagement.Models.User item in Model)
            {
            <tr>        
                <td>@item.id</td>
                <td>@item.name</td>              
                <td>@item.phNumber</td>
                <td>@item.email</td>               
                <td><a href="GetUserById/@item.id">View</a></td>
                <td><a href="DeleteUserById/@item.id">Delete</a></td>
            </tr>
            }
    </tbody>
</table>